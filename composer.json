{
    "name": "rkw/rkw-template",
    "type": "typo3-cms-extension",
    "description": "Templates and TypoScript for TYPO3",
    "homepage": "https://www.rkw-kompetenzzentrum.de",
    "license": ["GPL-2.0+"],
    "keywords": ["TYPO3 CMS", "RKW", "Templates"],
    "support": {
        "email": "developer@steffenkroggel.de"
    },
    "authors": [
        {
            "name": "Steffen Kroggel",
            "email": "developer@steffenkroggel.de",
            "homepage": "http://www.steffenkroggel.de",
            "role": "Developer"
        }
    ],
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/RKWKomZe/gridelements.git"
        }
    ],
    "require": {
        "php": ">=7.4",
        "typo3/cms-core": "~9.5.0",
        "gridelementsteam/gridelements" : "~9.8.0",
		"madj2k/core-extended": "~9.5.0",
		"madj2k/accelerator": "~9.5.0",
		"rkw/rkw-alerts": "~9.5.0 || dev-feature/upgrade9.5",
        "rkw/rkw-ajax": "~9.5.0 || dev-feature/upgrade9.5",
        "rkw/rkw-authors": "~9.5.0 || dev-feature/upgrade9.5",
        "rkw/rkw-basics": "~9.5.0 || dev-feature/upgrade9.5",
        "rkw/rkw-etracker": "~9.5.0 || dev-feature/upgrade9.5",
        "rkw/rkw-events": "~9.5.0 || dev-feature/upgrade9.5",
        "rkw/rkw-pdf2content": "~9.5.0 || dev-feature/upgrade9.5",
        "rkw/rkw-projects": "~9.5.0 || dev-feature/upgrade9.5",
        "rkw/rkw-registration": "~9.5.0 || dev-feature/upgrade9.5",
        "rkw/rkw-related": "~9.5.0 || dev-feature/upgrade9.5",
		"rkw/rkw-tools": "~9.5.0 || dev-feature/upgrade9.5"
	},

    "autoload": {
        "psr-4": {
            "RKW\\RkwTemplate\\": "Classes/"
        }
    },
    "extra": {
        "typo3/cms": {
            "extension-key": "rkw_template"
        }
    }
}
