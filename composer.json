{
    "name": "rkw/rkw-template",
    "type": "typo3-cms-extension",
    "description": "Templates and TypoScript for TYPO3",
    "homepage": "https://www.rkw-kompetenzzentrum.de",
    "license": ["GPL-2.0+"],
    "keywords": ["TYPO3 CMS", "RKW", "Templates"],
    "support": {
        "email": "developer@steffenkroggel.de"
    },
    "authors": [
        {
            "name": "Steffen Kroggel",
            "email": "developer@steffenkroggel.de",
            "homepage": "http://www.steffenkroggel.de",
            "role": "Developer"
        }
    ],
    "require": {
        "php": ">=7.4",
        "typo3/cms-core": "~9.5.0 || ~10.4.0",
        "gridelementsteam/gridelements" : "~11.1.0",
		"madj2k/t3-core-extended": "~9.5.4 || ~10.4.0",
		"madj2k/t3-accelerator": "~9.5.2 || ~10.4.0",
		"madj2k/t3-ajax-api": "~9.5.0 || ~10.4.0",
		"madj2k/t3-fe-register": "~9.5.0 || ~10.4.0",
		"rkw/rkw-alerts": "~9.5.0 || ~10.4.0",
        "rkw/rkw-authors": "~9.5.0 || ~10.4.0",
        "rkw/rkw-basics": "~9.5.0 || ~10.4.0",
        "rkw/rkw-etracker": "~9.5.0 || ~10.4.0",
        "rkw/rkw-events": "~9.5.0 || ~10.4.0",
        "rkw/rkw-pdf2content": "~9.5.0 || ~10.4.0",
        "rkw/rkw-projects": "~9.5.0 || ~10.4.0",
        "rkw/rkw-related": "~9.5.0 || ~10.4.0",
		"rkw/rkw-tools": "~9.5.0 || ~10.4.0"
	},
    "autoload": {
        "psr-4": {
            "RKW\\RkwTemplate\\": "Classes/"
        }
    },
    "extra": {
        "typo3/cms": {
            "extension-key": "rkw_template"
        }
    }
}
