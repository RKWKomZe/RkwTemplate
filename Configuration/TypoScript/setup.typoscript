plugin.tx_rkwtemplate {

	view {
		layoutRootPaths {
			0 = EXT:rkw_template/Resources/Private/Layouts/
			1 = {$plugin.tx_rkwtemplate.view.layoutRootPath}
		}
		templateRootPaths {
			0 = EXT:rkw_template/Resources/Private/Templates/
            1 = {$plugin.tx_rkwtemplate.view.templateRootPath}
        }
		partialRootPaths {
			0 = EXT:rkw_template/Resources/Private/Partials/
			1 = {$plugin.tx_rkwtemplate.view.partialRootPath}
        }
	}

	persistence {
		storagePid = {$plugin.tx_rkwtemplate.persistence.storagePid}
	}

	settings {


	}
}


lib.contentElement {
    layoutRootPaths.900 = {$plugin.tx_rkwtemplate.view.layoutRootPath}ContentElements
    templateRootPaths.900 = {$plugin.tx_rkwtemplate.view.templateRootPath}ContentElements
    partialRootPaths.900 = {$plugin.tx_rkwtemplate.view.partialRootPath}ContentElements
}

