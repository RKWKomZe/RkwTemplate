plugin.tx_rkwsocialcomments {
    view {
        // cat=plugin.tx_rkwsocialcomments/file; type=string; label=Path to template root (FE)
        templateRootPath = EXT:rkw_socialcomments/Resources/Private/Templates/
        // cat=plugin.tx_rkwsocialcomments/file; type=string; label=Path to template partials (FE)
        partialRootPath = EXT:rkw_socialcomments/Resources/Private/Partials/
        // cat=plugin.tx_rkwsocialcomments/file; type=string; label=Path to template layouts (FE)
        layoutRootPath = EXT:rkw_socialcomments/Resources/Private/Layouts/
    }

    persistence {
        storagePid = 636
    }

    settings {
        // cat=plugin.tx_rkwsocialcomments//a; type=boolean; label=Include jQuery?
        includeJQuery = 0

        // cat=plugin.tx_rkwsocialcomments//a; type=boolean; label=Include CSS
        includeCSS = 0

        // cat=plugin.tx_rkwsocialcomments//a; type=boolean; label=Use Ajax
        useAjax = 1

        // cat=plugin.tx_rkwsocialcomments//a; type=boolean; label=Use JS Pagination
        useJSPagination = 1

        // cat=plugin.tx_rkwsocialcomments//f; type=integer; label=PageType for Ajax
        pageTypeAjax = 1417692531

        // cat=plugin.tx_rkwsocialcomments//f; type=integer; label= List of pids where the MyComments-Plugin in inserted
        myCommentsPages = 743

        // cat=plugin.tx_rkwsocialcomments//f; type=integer; label=Comments per page
        commentsPerPage = 3

        // cat=plugin.tx_rkwsocialcomments//g; type=integer; label=Pid of login page
        loginPid = 2184

        // cat=plugin.tx_rkwsocialcomments//i; type=string; label=Reciever e-mail for spam reports
        recieverEmailAddressForSpamReports = service@rkw.de

        // cat=plugin.tx_rkwsocialcomments//i; type=string; label=Language key which defines the language of spam report-mails
        languageKeyForSpamReports = de
    }
}
