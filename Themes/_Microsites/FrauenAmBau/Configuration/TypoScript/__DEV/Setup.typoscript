config {

	# Cache deaktivieren (nur für Entwicklung)
	no_cache = 1
	admPanel = 0

	# Cache- Headers für intern gecachte Seiten
	sendCacheHeaders = 0

	# Compress and concatenate
	concatenateJs = 0
	compressJs = 0

	# Compress and concatenate
	concatenateCss = 0
	compressCss = 0

}

